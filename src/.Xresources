! URxvt settings
URxvt*font                     : xft:SauceCodePro\ Nerd\ Font:style=Bold:size=10:antialias=true
URxvt*scrollBar                : false
URxvt*internalBorder           : 10
URxvt*keysym*Shift-Control-V   : perl:clipboard:paste
URxvt*iso14755                 : false
URxvt*perl-ext-common          : default,clipboard
URxvt*perl-ext                 : url-select
URxvt*transparent              : true
URxvt*shading                  : 35
URxvt*blurRadius               : 10

! Global Terminal Colors
! special
*.foreground:   #a8a19f
*.background:   #1b1918
*.cursorColor:  #a8a19f

! black
*.color0:       #1b1918
*.color8:       #766e6b

! red
*.color1:       #d62323
*.color9:       #f22c40

! green
*.color2:       #afd700
*.color10:      #5ab738

! yellow
*.color3:       #d5911a
*.color11:      #d5911a

! blue
*.color4:       #407ee7
*.color12:      #407ee7

! magenta
*.color5:       #6666ea
*.color13:      #6666ea

! cyan
*.color6:       #00ad9c
*.color14:      #00ad9c

! white
*.color7:       #a8a19f
*.color15:      #f1efee
